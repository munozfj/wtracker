<% page_title "Graphs" %>

<div class="jumbotron">
  <h1>Graph </h1>

  <%= line_chart [
  {name: 'Weight', data: current_user.weights.map{|w| [w.date,w.weight]}},
  {name: 'Fat %', data: current_user.weights.map{|w| [w.date,w.fat_porcentage]}},
    ] %>

  <%= link_to "<button type='button' class='btn btn-default active'>
  <span class='glyphicon glyphicon-chevron-left'></span> </button>".html_safe , panel_path %>
        
</div>